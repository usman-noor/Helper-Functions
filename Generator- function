def rmotr_range(start, end, step=1):
    current = start
    while current < end:
        num = current
        current += step
        yield num



gen = iter(rmotr_range(start=0, end=3))
print(next(gen))
print(next(gen))
print(next(gen))
print(next(gen))
print(next(gen))

i  = iter(rmotr_range(1,10,1))
print (next(i))

for i in rmotr_range(1,10,1):
  print(i)



def create_list(n):
    current  = 0 
    while current < n 
    yield current 
    current += 1
    
list(create_list(3))



